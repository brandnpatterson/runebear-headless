{
  "name": "runebear",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "npm run stylelint && react-app-rewired start",
    "build": "react-app-rewired build && npm run create-cname",
    "create-cname": "echo 'test-runebear.surge.sh' > build/CNAME",
    "deploy": "npm run build && surge build",
    "stylelint": "stylelint src/**/*.jsx",
    "test": "react-app-rewired test --env=jsdom"
  },
  "dependencies": {
    "axios": "^0.16.2",
    "prop-types": "^15.5.10",
    "react": "^16.2.0",
    "react-dom": "^16.4.0",
    "react-router-dom": "^4.1.2",
    "styled-components": "^3.2.5"
  },
  "devDependencies": {
    "react-app-rewire-hot-loader": "^1.0.1",
    "react-app-rewired": "^1.5.2",
    "react-hot-loader": "^4.3.3",
    "react-scripts": "1.1.0",
    "stylelint": "^9.2.1",
    "stylelint-config-recommended": "^2.1.0",
    "stylelint-config-sass-guidelines": "^5.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.3.1"
  }
}
